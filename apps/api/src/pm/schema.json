{
  "topNode": "doc",
  "nodes": {
    "paragraph": { "content": "inline*", "group": "block", "parseDOM": [{ "tag": "p" }] },
    "doc": { "content": "block+" },
    "text": { "group": "inline" },
    "heading": {
      "content": "inline*",
      "group": "block",
      "defining": true,
      "attrs": { "level": { "default": 1 } },
      "parseDOM": [
        { "tag": "h1", "attrs": { "level": 1 } },
        { "tag": "h2", "attrs": { "level": 2 } },
        { "tag": "h3", "attrs": { "level": 3 } }
      ]
    },
    "hardBreak": { "group": "inline", "inline": true, "selectable": false, "parseDOM": [{ "tag": "br" }] },
    "horizontalRule": { "group": "block", "parseDOM": [{ "tag": "hr" }] },
    "blockquote": {
      "content": "paragraph+",
      "group": "block",
      "defining": true,
      "parseDOM": [{ "tag": "blockquote" }]
    },
    "bulletList": { "content": "listItem+", "group": "block list", "parseDOM": [{ "tag": "ul" }] },
    "orderedList": {
      "content": "listItem+",
      "group": "block list",
      "attrs": { "start": { "default": 1 }, "type": { "default": null } },
      "parseDOM": [{ "tag": "ol" }]
    },
    "listItem": {
      "content": "paragraph (paragraph | bulletList | orderedList)*",
      "defining": true,
      "parseDOM": [{ "tag": "li" }]
    },
    "callout": {
      "content": "paragraph+",
      "group": "block",
      "defining": true,
      "attrs": { "emoji": { "default": "alert" } },
      "parseDOM": [{ "tag": "node-view[data-node-view-type=callout]" }]
    },
    "embed": {
      "group": "block",
      "draggable": true,
      "attrs": {
        "id": { "default": null },
        "url": { "default": null },
        "title": { "default": null },
        "description": { "default": null },
        "thumbnailUrl": { "default": null },
        "html": { "default": null }
      },
      "parseDOM": [{ "tag": "node-view[data-node-view-type=embed]" }]
    },
    "image": {
      "group": "block",
      "draggable": true,
      "attrs": {
        "id": { "default": null },
        "url": { "default": null },
        "ratio": { "default": null },
        "placeholder": { "default": null }
      },
      "parseDOM": [{ "tag": "node-view[data-node-view-type=image]" }]
    },
    "file": {
      "group": "block",
      "draggable": true,
      "attrs": {
        "id": { "default": null },
        "name": { "default": null },
        "size": { "default": null },
        "url": { "default": null }
      },
      "parseDOM": [{ "tag": "node-view[data-node-view-type=file]" }]
    }
  },
  "marks": {
    "link": {
      "inclusive": true,
      "attrs": {
        "href": { "default": null },
        "target": { "default": "_blank" },
        "rel": { "default": "noopener noreferrer nofollow" },
        "class": {
          "default": "c-var(--usersite-theme-color) fw-[500] text-decoration-line-underline td-c-var(--usersite-theme-color)/40 tu-o-2px cursor-pointer hover:td-c-var(--usersite-theme-color)/80 hover:td-t-2px"
        }
      },
      "parseDOM": [{ "tag": "a[href]" }]
    },
    "textStyle": { "attrs": { "color": { "default": null } }, "parseDOM": [{ "tag": "span" }] },
    "bold": { "parseDOM": [{ "tag": "strong" }, { "tag": "b" }, { "style": "font-weight" }] },
    "italic": { "parseDOM": [{ "tag": "em" }, { "tag": "i" }, { "style": "font-style=italic" }] },
    "strike": {
      "parseDOM": [
        { "tag": "s" },
        { "tag": "del" },
        { "tag": "strike" },
        { "style": "text-decoration", "consuming": false }
      ]
    },
    "underline": { "parseDOM": [{ "tag": "u" }, { "style": "text-decoration", "consuming": false }] },
    "code": { "excludes": "_", "code": true, "parseDOM": [{ "tag": "code" }] }
  }
}
